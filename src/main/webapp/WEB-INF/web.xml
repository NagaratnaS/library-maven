<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddLibrary</servlet-name>
  	<display-name>AddLibrary</display-name>
  	<description></description>
  	<servlet-class>com.wolken.wolkenapp.servlet.AddLibrary</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetAllLibrary</servlet-name>
  	<display-name>GetAllLibrary</display-name>
  	<description></description>
  	<servlet-class>com.wolken.wolkenapp.servlet.GetAllLibrary</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateLibraryServlet</servlet-name>
  	<display-name>UpdateLibraryServlet</display-name>
  	<description></description>
  	<servlet-class>com.wolken.wolkenapp.servlet.UpdateLibraryServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteLibraryServlet</servlet-name>
  	<display-name>DeleteLibraryServlet</display-name>
  	<description></description>
  	<servlet-class>com.wolken.wolkenapp.servlet.DeleteLibraryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddLibrary</servlet-name>
  	<url-pattern>/AddLibrary</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetAllLibrary</servlet-name>
  	<url-pattern>/GetAllLibrary</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateLibraryServlet</servlet-name>
  	<url-pattern>/UpdateLibraryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteLibraryServlet</servlet-name>
  	<url-pattern>/DeleteLibraryServlet</url-pattern>
  </servlet-mapping>
</web-app>
